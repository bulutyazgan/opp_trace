{
  "manifest_version": 3,
  "name": "LumedIn Analyzer",
  "version": "1.0.3",
  "description": "AI-powered event attendee analysis and networking insights for Luma events",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://lu.ma/*",
    "https://luma.com/*",
    "https://*.lu.ma/*",
    "https://*.luma.com/*",
    "http://localhost:3000/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://lu.ma/*", "https://luma.com/*", "https://*.lu.ma/*", "https://*.luma.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
